{
   "type": "entity",
   "mixins": [
      "stonehearth_ace:trees:walnut:small"
   ],
   "components": {
      "mixintypes": {
         "stonehearth:commands": "remove",
         "stonehearth:entity_forms": "remove",
         "stonehearth_ace:transform": "remove"
      },
      "stonehearth:resource_node": {
         "resource_loot_table": {
            "entries": {
               "optional": {
                  "mixintypes": {
                     "items": "override"
                  },
                  "items": {
                     "none": {
                        "weight": 1
                     }
                  }
               }
            }
         }
      },
      "stonehearth:renewable_resource_node": {
         "category": "farming",
         "mixintypes": {
            "harvest_overlay_effect": "remove"
         },
         "renewal_time": "4d+1h",
         "resource_loot_table": {
            "entries": {
               "optional": {
                  "mixintypes": {
                     "items": "override"
                  },
                  "items": {
                     "none": {
                        "weight": 1
                     }
                  }
               }
            }
         }
      },
      "stonehearth:loot_drops": {
         "entries": {
            "beehive": {
               "num_rolls": {
                  "min": 0,
                  "max": 0
               }
            }
         }
      },
      "mob": {
         "mixintypes": {
            "align_to_grid": "remove"
         },
         "model_origin": { "x": -0.5, "y": 0, "z": -0.5 },
         "region_origin": { "x": 0.5, "y": 0, "z": 0.5 }
      }
   },
   "entity_data": {
      "stonehearth_ace:evolve_modifiers": {
         "allow_manual_stunting": false
      },
      "mixintypes": {
         "stonehearth_ace:transform_data": "remove",
         "stonehearth_ace:replant_data": "remove"
      },
      "stonehearth:evolve_data": {
         "next_stage": "stonehearth_ace:crops:walnut_tree_crop:medium"
      }
   }
}
